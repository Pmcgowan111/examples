{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build2": {
      "dependsOn": ["^build2"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
